{
"name":"Pashua",
"type":"utility",
"versions":{
  "default":{
    "version":"default",
    "get-method":"dmg",
    "invoke": "null",
    "files":{
     "1":"http://www.bluem.net/files/Pashua.dmg"
     },
    "install":{
      "1":"hdiutil attach -nobrowse -quiet __FILE__",
